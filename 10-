#include <stdio.h>
void swapWithVariable(int *a, int *b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}

void swapWithoutVariable(int *a, int *b) {
    *a = *a + *b;
    *b = *a - *b;
    *a = *a - *b;
}

int main() {
    int num1, num2;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    swapWithVariable(&num1, &num2);
    printf("After swapping using a third variable:\nnum1 = %d, num2 = %d\n", num1, num2);
    num1 = 10;
    num2 = 20;
    swapWithoutVariable(&num1, &num2);
    printf("After swapping without using a third variable:\nnum1 = %d, num2 = %d\n", num1, num2);
    return 0;
}

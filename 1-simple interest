
#include<stdio.h>
int main()
{int pamount,rate,time;
 printf("enter the princial amount:");
scanf(" %d",& pamount);
printf("enter the rate:");
scanf(" %d",& rate);
printf("enter the time:");
scanf(" %d",& time);

int simpleintrest=(pamount*rate*time)/100;
 printf("the simple intrest is %d\n",simpleintrest);
 printf("end amount is  %d",simpleintrest);
 return 0;
}

